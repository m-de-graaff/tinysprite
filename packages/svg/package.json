{
  "name": "@tinysprites/svg",
  "version": "0.1.0",
  "description": "SVG export support for tinysprites (optional)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@tinysprites/core": "workspace:*"
  },
  "keywords": ["sprites", "svg", "js13k", "optional"],
  "license": "MIT"
}
