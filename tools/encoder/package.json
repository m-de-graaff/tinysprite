{
  "name": "@tinysprites/encoder",
  "version": "0.1.0",
  "description": "Encoder CLI for tinysprites",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tinysprites-encode": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "pngjs": "^7.0.0",
    "commander": "^11.1.0"
  },
  "peerDependencies": {
    "@tinysprites/core": "workspace:*"
  },
  "keywords": ["sprites", "encoder", "cli", "js13k"],
  "license": "MIT"
}